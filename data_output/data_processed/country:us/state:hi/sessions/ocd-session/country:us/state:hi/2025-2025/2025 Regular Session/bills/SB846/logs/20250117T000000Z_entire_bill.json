  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits the Governor or a Mayor from suspending requests for public records or vital statistics during a declared state of emergency.  Allows for a reasonable delay in an agency's response to a request as a result of extenuating circumstances."
    }
  ],
