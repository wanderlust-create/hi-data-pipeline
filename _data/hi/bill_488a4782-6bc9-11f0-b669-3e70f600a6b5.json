{"legislative_session": "2025", "identifier": "SB1192", "title": "RELATING TO SCHOOL MEALS.", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": ["DOE", "Notification", "Prohibited", "Public Identification", "School Meals", "Stigmatization", "Supplemental Nutrition Assistance Program", "Unpaid Account Balances", "Zero or Negative Account Balances"], "abstracts": [{"note": "description", "abstract": "Prohibits the denial of a school meal to a student based on the student's ability to pay.  Requires the Department of Education and department schools to certify that all students eligible for certain programs receive free or reduced-price meals.  Prohibits a department school from publicly identifying or stigmatizing a student.  Requires the school to directly inform a parent or guardian regarding a student's unpaid meal fund account balance."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Referred to EDU, WAM/JDC.", "date": "2025-01-27", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": [{"name": "Education", "entity_type": "organization", "organization_id": null}, {"name": "Ways and Means", "entity_type": "organization", "organization_id": null}, {"name": "Judiciary", "entity_type": "organization", "organization_id": null}]}, {"description": "Passed First Reading.", "date": "2025-01-21", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Introduced.", "date": "2025-01-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["introduction"], "related_entities": []}], "sponsorships": [{"name": "GABBARD", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"name\": \"GABBARD\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "SB1192", "links": [{"url": "https://data.capitol